    <!-- //lined-icons -->
    <!-- chart -->
    <script src="../../Content/js/jquery-1.10.2.min.js"></script>
    <script src="../../Content/js/Chart.js"></script>
    <script src="../../Content/js/Global.js"></script>
    <!-- //chart -->
    <!--animate-->
