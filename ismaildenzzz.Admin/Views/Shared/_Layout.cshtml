            <div class="logo">
                <h2><a href="/Home/Index">ismaildenzzz <span>Admin</span></a></h2>
            </div>
            <div class="logo-icon text-center">
                <a href="/Home/Index"><i class="lnr lnr-home"></i> </a>
            </div>

            <!--logo and iconic logo end-->
            @Html.Partial("_LeftMenu")
        </div>
        <!-- left side end-->
        <!-- main content start-->
        <div class="main-content">
            <!-- header-starts -->
            @Html.Partial("_HeaderSection")
            <!-- //header-ends -->
            @RenderBody()
            <!--body wrapper end-->
        </div>
        <!--footer section start-->
        <footer>
            <p>&copy 2015 Easy Admin Panel. All Rights Reserved | Design by <a href="https://w3layouts.com/" target="_blank">w3layouts.</a></p>
